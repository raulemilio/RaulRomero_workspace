#include "main.h"
#include <stdbool.h>

typedef enum{
	   BUTTON_UP,
	   BUTTON_FALLING,
	   BUTTON_DOWN,
	   BUTTON_RAISING,
  } debounceState_t;

typedef bool bool_t;

void debounceFSM_init(void);		// Se carga el estado inicial de la FSM
void debounceFSM_update(void);	// Actualizaci√≥n de la FSM
void buttonPressed(void);			// Invierte el estado del LED2
void buttonReleased(void);		// Invierte el estado del LED1
bool_t readKey( delay_t * delay );
